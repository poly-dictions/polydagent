{
  "buildCommand": "",
  "outputDirectory": ".",
  "rewrites": [
    { "source": "/markets", "destination": "/markets/index.html" },
    { "source": "/markets/", "destination": "/markets/index.html" },
    { "source": "/whales", "destination": "/whales/index.html" },
    { "source": "/whales/", "destination": "/whales/index.html" },
    { "source": "/arbitrage", "destination": "/arbitrage/index.html" },
    { "source": "/arbitrage/", "destination": "/arbitrage/index.html" },
    { "source": "/alerts", "destination": "/alerts/index.html" },
    { "source": "/alerts/", "destination": "/alerts/index.html" },
    { "source": "/roadmap", "destination": "/roadmap/index.html" },
    { "source": "/roadmap/", "destination": "/roadmap/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" }
      ]
    }
  ]
}
